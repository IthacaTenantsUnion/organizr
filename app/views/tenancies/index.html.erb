<p id="notice"><%= notice %></p>

<h1>Tenancies</h1>

<table>
  <thead>
    <tr>
      <th>Landlord</th>
      <th>Unit</th>
      <th>Rent</th>
      <th>Date</th>
      <th>Rating</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tenancies.each do |tenancy| %>
      <tr>
        <td><%= tenancy.user %></td>
        <td><%= tenancy.landlord.name %></td>
        <td></td>
        <td></td>
        <td><%= render partial: "tenancy_rating", object: tenancy %></td>
        <td><%= link_to 'Show', tenancy %></td>
        <td><%= edit_link(edit_tenancy_path(tenancy)) if can?(:edit, tenancy) %></td>
        <td><%= destroy_link(tenancy) if can?(:destroy, tenancy) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tenancy', new_tenancy_path %>
